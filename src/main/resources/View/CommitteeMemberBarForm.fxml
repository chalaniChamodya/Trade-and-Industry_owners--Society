<AnchorPane prefHeight="60.0" prefWidth="819.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.Trade_and_Industrial_owners_Society.Controller.CommitteeMemberBarFormController">
   <children>
      <HBox layoutX="26.0" layoutY="13.0" prefHeight="47.0" prefWidth="772.0">
         <children>
            <Label fx:id="comMemId" prefHeight="47.0" prefWidth="204.0" text="com mem id" textFill="#533710">
