<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="barPane" prefHeight="60.0" prefWidth="1493.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.Trade_and_Industrial_owners_Society.Controller.MemberBarFormController">
   <children>
      <HBox layoutX="44.0" layoutY="14.0" prefHeight="46.0" prefWidth="1467.0">
         <children>
            <Label fx:id="member_id" prefHeight="60.0" prefWidth="282.0" text="member_id" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <Label fx:id="name" layoutX="10.0" layoutY="10.0" prefHeight="46.0" prefWidth="264.0" text="Name" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <Label fx:id="contact_no" layoutX="10.0" layoutY="10.0" prefHeight="46.0" prefWidth="263.0" text="contact No" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <Label fx:id="business_type" layoutX="522.0" layoutY="10.0" prefHeight="46.0" prefWidth="242.0" text="Business Type" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <Label fx:id="nic" layoutX="741.0" layoutY="10.0" prefHeight="60.0" prefWidth="254.0" text="NIC" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <ImageView fx:id="btnDelete" fitHeight="30.0" fitWidth="30.0" onMouseClicked="#btnDeleteOnAction">
               <image>
                  <Image url="@../Assets/Icons/delete.png" />
               </image>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ImageView>
            <ImageView fx:id="btnUpdate" fitHeight="30.0" fitWidth="30.0" onMouseClicked="#btnUpdateOnAction">
               <image>
                  <Image url="@../Assets/Icons/update.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </children>
</AnchorPane>
