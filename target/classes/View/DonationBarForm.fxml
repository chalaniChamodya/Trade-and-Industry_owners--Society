<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="60.0" prefWidth="819.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.Trade_and_Industrial_owners_Society.Controller.DonationBarFormController">
   <children>
      <HBox layoutX="18.0" prefHeight="60.0" prefWidth="780.0">
         <children>
            <Label fx:id="donationId" prefHeight="60.0" prefWidth="168.0" text="Id" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <Label fx:id="date" layoutX="10.0" layoutY="10.0" prefHeight="60.0" prefWidth="216.0" text="Date" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <Label fx:id="amount" layoutX="10.0" layoutY="10.0" prefHeight="60.0" prefWidth="315.0" text="Amount" textFill="#533710">
               <font>
                  <Font name="SansSerif Regular" size="19.0" />
               </font>
            </Label>
            <ImageView fx:id="btnDelete" fitHeight="30.0" fitWidth="30.0" onMouseClicked="#btnDeleteOnAction">
               <image>
                  <Image url="@../Assets/Icons/delete.png" />
               </image>
               <HBox.margin>
                  <Insets top="12.0" />
               </HBox.margin>
            </ImageView>
            <ImageView fx:id="btnUpdate" fitHeight="30.0" fitWidth="30.0" onMouseClicked="#btnUpdateOnAction">
               <image>
                  <Image url="@../Assets/Icons/update.png" />
               </image>
               <HBox.margin>
                  <Insets left="10.0" top="12.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
   </children>
</AnchorPane>
